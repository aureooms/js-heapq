{
  "name": "@aureooms/js-heapq",
  "description": "Python's heap and priority queue library for JavaScript",
  "version": "3.1.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-heapq/issues"
  },
  "dependencies": {
    "@aureooms/js-compare": "^1.4.5",
    "@aureooms/js-error": "^3.0.0",
    "@aureooms/js-itertools": "^3.1.1"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "6.24.1",
    "codeclimate-test-reporter": "0.5.1",
    "coveralls": "2.13.3",
    "esdoc": "1.1.0",
    "nyc": "11.9.0"
  },
  "homepage": "https://aureooms.github.io/js-heapq",
  "keywords": [
    "heap",
    "heapq",
    "javascript",
    "js",
    "priority",
    "queue"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-heapq.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepublish": "npm run build",
    "test": "ava ./test/src"
  }
}
